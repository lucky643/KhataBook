<%- include('./partials/header.ejs') %>
     <div class="createUser bg-rose-100 border-2 divide-slate-700 shadow-xl w-1/3 absolute top-1/2 left-1/2 -translate-x-1/2 -translate-y-1/2 rounded-md p-4">
          <h1 class="mb-5">Create New Account</h1>
          <form class="flex flex-col gap-4" autocomplete="off" action="/register" method="post">
               <input class="rounded-md bg-rose-50 px-2 py-3 outline-none" type="email" placeholder="Email" name="email">
               <input class="rounded-md bg-rose-50 px-2 py-3 outline-none" type="text" placeholder="User Name" name="username">
               <input class="rounded-md bg-rose-50 px-2 py-3 outline-none" type="text" placeholder="Full Name" name="name">
               <input class="rounded-md bg-rose-50 px-2 py-3 outline-none" type="password" placeholder="Password" name="password">
               <input class="bg-blue-400 rounded-md px-2 py-1 w-fit" type="submit" value="Create">
               <a class="text-blue-600 text-sm" href="/">have an account, login here.</a>

          </form>
     </div>
<%- include('./partials/footer.ejs') %>
